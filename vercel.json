{
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist"
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	],
	"env": {
		"VITE_CLERK_PUBLISHABLE_KEY": "@vite_clerk_publishable_key",
		"VITE_API_URL": "@vite_api_url"
	}
}
